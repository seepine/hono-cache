{
  "name": "@seepine/hono-cache",
  "version": "0.0.1",
  "description": "cache for hono",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "keywords": [
    "hono",
    "cache",
    "redis"
  ],
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seepine/hono-cache.git"
  },
  "homepage": "https://github.com/seepine/hono-cache",
  "author": "seepine (https://github.com/seepine)",
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "format": "prettier --write src"
  },
  "peerDependencies": {
    "hono": ">=4.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@vitest/coverage-v8": "~3.0.9",
    "hono": "^4.10.3",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "typescript": "^5.8.2",
    "vitest": "~3.0.9"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@keyv/redis": "^5.1.3",
    "cacheable": "^2.1.1"
  }
}